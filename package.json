  "homepage": "https://github.com/stefanvermaas/factory-builder",
  "main": "dist/factory-builder.js",
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "lint": "eslint ./src/**",
    "lint:fix": "yarn run lint --fix",
    "prepare": "yarn run build",
    "prerelease": "yarn run lint:fix && yarn run test:coverage && yarn run build",
    "release": "npm publish",
    "start": "yarn run build:watch start",
    "test": "yarn run jest",
    "test:watch": "yarn run jest --watch",
    "test:coverage": "jest --coverage --colors"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.4.4",
